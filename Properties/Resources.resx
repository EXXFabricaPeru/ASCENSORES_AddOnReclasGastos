  <data name="ListarAsientos" xml:space="preserve">
    <value>SELECT 
	'N' "Select",
	T0."TransId",
	T1."Line_ID",
	T1."Account",
	T2."AcctName",	
	T1."ProfitCode",
	(SELECT "PrcName" FROM OPRC T3 WHERE T3 ON T1."ProfitCode" = T3."PrcCode") "ProfitCodeName",
	T1."OcrCode2"
	(SELECT "PrcName" FROM OPRC T3 WHERE T3 ON T1."OcrCode2" = T3."PrcCode") "OcrCode2Name",
	T1."OcrCode3"
	(SELECT "PrcName" FROM OPRC T3 WHERE T3 ON T1."OcrCode3" = T3."PrcCode") "OcrCode3Name",
	T1."OcrCode4"
	(SELECT "PrcName" FROM OPRC T3 WHERE T3 ON T1."OcrCode4" = T3."PrcCode") "OcrCode4Name",
	T1."OcrCode5"
	(SELECT "PrcName" FROM OPRC T3 WHERE T3 ON T1."OcrCode5" = T3."PrcCode") "OcrCode5Name",
	T3."PrcName",
	T1."Debit" - T1."Credit" "Total ML",
	T1."FCDebit" - T1."FCCredit" "Total ME",
	T1."SYSDeb" - T1."SYSCred" "Total MS",
	T1."Project",
	T1."Ref1",
	T1."Ref2",
	T1."Ref3Line"
FROM OJDT T0
INNER JOIN JDT1 T1 ON T0."TransId" = T1."TransId"
INNER JOIN OACT T2 ON T1."Account" = T2."AcctCode"
--INNER JOIN OPRC T3 ON T1."{0}" = T3."PrcCode"
WHERE T0."RefDate" BETWEEN '{1}' AND '{2}'
AND T1."Account" IN ({3})
AND T1."{0}" IN ({4})
ORDER BY T1."Account", T1."{0}", T0."TransId"</value>
  </data>
  <data name="ListarCCGasto" xml:space="preserve">
    <value>SELECT 'N' "Select", "PrcCode", "PrcName" FROM OPRC WHERE "DimCode" = {0}  AND "U_EXX_ADRG_TIPOCC" = '1' ORDER BY "PrcCode"</value>
  </data>
  <data name="ListarCCProductivo" xml:space="preserve">
    <value>SELECT "PrcCode", "PrcName", 0 "Peso" FROM OPRC WHERE "DimCode" = {0} AND "U_EXX_ADRG_TIPOCC" = '2' ORDER BY "PrcCode"</value>
  </data>
  <data name="ListarCuentaGasto" xml:space="preserve">
    <value>SELECT 'N' "Select", "AcctCode", "AcctName" FROM OACT WHERE "Levels" = 7 AND "U_EXX_ADRG_CTAGASTO" = 'Y' ORDER BY "AcctCode"</value>
  </data>
  <data name="ListarHistorico" xml:space="preserve">
    <value>SELECT 
	'N' "Col_0",
	"U_EXX_ADRG_FECHAE" "Col_1",
	"U_EXX_ADRG_TRANSID" "Col_2",
	"U_EXX_ADRG_EST" "Col_3",
	"U_EXX_ADRG_FECHAC" "Col_4",
	"U_EXX_ADRG_GLOSA" "Col_5",
	"U_EXX_ADRG_FECHAA" "Col_6"
	"U_EXX_ADRG_TRANSIDA" "Col_7",
	"Code"
FROM "@EXX_ADRG_HIST"
WHERE "U_EXX_ADRG_EST" IN ({0})</value>
